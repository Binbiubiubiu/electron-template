  "scripts": {
    "dev": "cross-env NODE_ENV=develepment TS_NODE_PROJECT=\"scripts/tsconfig-for-webpack-config.json\" ts-node scripts/dev.ts",
    "build": "cross-env NODE_ENV=develepment TS_NODE_PROJECT=\"scripts/tsconfig-for-webpack-config.json\" ts-node scripts/build.ts",
    "watch": "cross-env NODE_ENV=develepment TS_NODE_PROJECT=\"scripts/tsconfig-for-webpack-config.json\" webpack serve -c scripts/webpack.config.renderer.ts",
