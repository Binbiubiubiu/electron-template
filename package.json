{
  "name": "electron-template",
  "version": "0.0.1",
  "description": "a electron template",
  "scripts": {
    "dev": "npm run watch",
    "dev:electron": "electron app",
    "watch": "cross-env NODE_ENV=develepment TS_NODE_PROJECT=\"scripts/tsconfig-for-webpack-config.json\" webpack serve  -c scripts/webpack.config.ts",
    "build": "cross-env NODE_ENV=production TS_NODE_PROJECT=\"scripts/tsconfig-for-webpack-config.json\" webpack -c scripts/webpack.config.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint:style": "stylelint \"src/*.{css,less}\"",
    "fix:style": "stylelint --fix \"src/*.{css,less}\"",
    "pack": "electron-builder build --dir --publish=always",
    "release": "electron-builder build --publish=always"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Simon-Bin/electron-template.git"
  },
  "keywords": [],
  "author": "Simon-Bin<2632003786@qq.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Simon-Bin/electron-template/issues"
  },
  "homepage": "https://github.com/Simon-Bin/electron-template#readme",
  "devDependencies": {
    "@babel/core": "^7.13.14",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.13.15",
    "@babel/preset-react": "^7.13.13",
    "@babel/preset-typescript": "^7.13.0",
    "@types/copy-webpack-plugin": "^6.4.1",
    "@types/electron-devtools-installer": "^2.2.0",
    "@types/loadable__component": "^5.13.3",
    "@types/mini-css-extract-plugin": "^1.4.1",
    "@types/node": "^14.14.37",
    "@types/progress-bar-webpack-plugin": "^2.1.1",
    "@types/react-dom": "^17.0.3",
    "@types/react-router-dom": "^5.1.7",
    "@types/webpack": "^5.28.0",
    "@types/webpack-dev-server": "^3.11.3",
    "@typescript-eslint/eslint-plugin": "^4.21.0",
    "@typescript-eslint/parser": "^4.21.0",
    "autoprefixer": "^10.2.5",
    "babel-loader": "^8.2.2",
    "babel-plugin-import": "^1.13.3",
    "concurrently": "^6.0.1",
    "copy-webpack-plugin": "^8.1.1",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.0",
    "electron": "^12.0.2",
    "electron-builder": "^22.10.5",
    "electron-devtools-installer": "3.1.0",
    "electron-rebuild": "^2.3.5",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.23.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "fork-ts-checker-webpack-plugin": "^6.2.0",
    "html-webpack-plugin": "^5.3.1",
    "less": "^4.1.1",
    "less-loader": "^8.0.0",
    "mini-css-extract-plugin": "^1.4.1",
    "postcss": "^8.2.9",
    "postcss-loader": "^5.2.0",
    "prettier": "^2.2.1",
    "progress-bar-webpack-plugin": "^2.1.0",
    "style-loader": "^2.0.0",
    "stylelint": "^13.12.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^21.0.0",
    "stylelint-order": "^4.1.0",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.4",
    "webpack": "^5.31.0",
    "webpack-cli": "^4.6.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.7.3"
  },
  "dependencies": {
    "@loadable/component": "^5.14.1",
    "antd": "^4.15.0",
    "core-js": "^3.10.1",
    "electron-is-dev": "^2.0.0",
    "electron-log": "^4.3.4",
    "electron-updater": "^4.3.8",
    "mobx": "^6.1.8",
    "mobx-react-lite": "^3.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0"
  },
  "browserslist": [
    "chrome 80"
  ],
  "engines": {
    "node": ">=12.0.0"
  },
  "build": {
    "appId": "com.example.electronTemplate",
    "directories": {
      "output": "dist",
      "app": "app"
    },
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ],
      "internetEnabled": true
    },
    "mac": {
      "target": "default",
      "category": "public.app-category.tools",
      "icon": "build/icons/icon.icns"
    },
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        },
        "zip"
      ],
      "icon": "build/icons/icon.ico"
    },
    "linux": {
      "target": "AppImage",
      "icon": "build/icons"
    },
    "nsis": {
      "artifactName": "${productName}-Setup-${version}.${ext}",
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "allowElevation": true,
      "perMachine": true,
      "runAfterFinish": true,
      "deleteAppDataOnUninstall": true
    },
    "publish": [
      {
        "provider": "generic",
        "url": "http://localhost:3000/autoUpdate"
      }
    ]
  }
}
